<h1>Articles#new</h1>
<p>Find me in app/views/articles/new.html.erb</p>
<%= link_to "Click here to move Index ", welcome_index_path %><br></br>
<%= link_to "View Data", articles_path %><br></br>
</br></br>

<%= form_for :articles , url: articles_path do |f| %>
  <p>
    <%= f.label :Enter_Title %><br>
    <%= f.text_field :title %>
  </p>

  <p>
    <%= f.label :Enter_Text %><br>
    <%= f.text_area :text %>
  </p>

  <p>
    <%= f.submit %>
  </p>
<% end %>
